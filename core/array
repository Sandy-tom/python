import array

arr = array.array('u',['a','b','c','d'])    // arry in char
for i in arr:
      print(i)
     
      //program 3:
      from array import *

arr = array('i',[1,2,3,4,5,6])
fun = array(arr.typecodetype,(a for a in arr))


for i in fun:
      print(i)
